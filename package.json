{
  "name": "steemSTEM",
  "version": "1.0.0",
  "engines": {
    "node": "9.11.1"
  },
  "bin": "./server",
  "main": "./server",
  "private": true,
  "scripts": {
    "lint": "node ./scripts/eslint/index.js all",
    "linc": "node ./scripts/eslint/index.js",
    "prettier": "node ./scripts/prettier/tasks/formatFiles.js",
    "prettier-all": "node ./scripts/prettier/tasks/formatFiles.js all",
    "prettier-check": "node ./scripts/prettier/tasks/checkFiles.js",
    "prettier-check-all": "node ./scripts/prettier/tasks/checkFiles.js all",
    "prettier-file": "node ./scripts/prettier/tasks/formatFile.js",
    "dev-server": "run-p dev-server:auth-server dev-server:client",
    "dev-server:client": "webpack-dev-server --config ./webpack/webpack-dev-server.js",
    "dev-server:auth-server": "node ./src/server/auth-server.js",
    "start": "cross-env NODE_ENV=production npm run run:server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint-autofix": "eslint --fix \"src/**/*.js\"",
    "run:server": "nodemon --watch busy.server.js busy.server.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.server.config.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --config ./webpack/webpack.server.config.js",
    "build:client": "webpack -p --config ./webpack/webpack.prod.config.js",
    "build": "cross-env NODE_ENV=production npm run build:client && npm run build:server",
    "heroku-prebuild": "npm i -g pm2 && pm2 install pm2-logrotate",
    "heroku-postbuild": "npm run build"
  },
  "pre-commit": [
    "linc",
    "prettier-check",
    "test"
  ],
  "dependencies": {
    "antd": "^3.0.0",
    "autoprefixer": "^8.3.0",
